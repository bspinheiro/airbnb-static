<div class="container text-center padded">
  <h1>Our Team</h1>
  <ul class="list-inline text-center">
  <% ["anne", "seb", "romain"].each do |member| %>
    <li>
      <%= image_tag "#{member}.jpg", class: "avatar-large avatar-bordered" %>
      <h3><%= member.capitalize %></h3>
    </li>
  <% end %>
  </ul>
</div>

<div class="container text-center padded">
  <a href="mailto:contact@airbnb.com"><i class="fa fa-envelope"></i></a>
  <%= link_to "contact@airbnb.com", "mailto:contact@airbnb.com"%>
</div>
